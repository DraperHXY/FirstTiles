<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.draper.dao.ProfessionMapper">

    <select id="selectByDirection" resultType="com.draper.entity.Profession">
        SELECT *
        FROM profession
        WHERE development_direction = #{direction};
    </select>

</mapper>